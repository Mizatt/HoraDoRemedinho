<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="projetodiego.unimep.com.br.projetodiego">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hora do Remedinho"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ExclusaoDeRegistro"></activity>
        <activity android:name=".CancelamentoCadastroRemedio" />
        <activity android:name=".TelaConfiguracao" />
        <activity android:name=".TelaPrincipal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RemediosCadastrados" />
        <activity android:name=".CadastroDeRemedios" />
        <activity android:name=".sucessoCadastroRemedinho" />
        <activity android:name=".sucessoCadastroUsuario" />
        <activity android:name=".PopErro" />
        <activity android:name=".Login" />
        <activity android:name=".CadastroUsuario" />
    </application>

</manifest>